{
  "name": "ipfs-desktop",
  "private": true,
  "version": "1.0.0",
  "productName": "IPFS Desktop",
  "description": "IPFS Native Application",
  "main": "out/index.js",
  "scripts": {
    "start": "electron -r @babel/register src/index.js",
    "lint": "standard",
    "compile": "babel src --out-dir out --copy-files",
    "pack": "npm run compile && electron-builder --dir",
    "icons": "svgr -d src/icons node_modules/ipfs-css/icons && standard --fix src/icons/*.js",
    "build": "npm run compile && electron-builder -p never"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "ipfs",
    "electron",
    "station",
    "desktop",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop"
  },
  "author": "IPFS",
  "contributors": [
    "Kristoffer Str√∂m <kristoffer@rymdkoloni.se>",
    "David Dias <daviddias@ipfs.io>",
    "Juan Benet <juan@ipfs.io>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Henrique Dias <hacdias@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs-shipyard/ipfs-desktop/issues"
  },
  "homepage": "https://github.com/ipfs-shipyard/ipfs-desktop",
  "build": {
    "appId": "io.ipfs.desktop",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/cli": "^2.4.1",
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "auto-launch": "^5.0.5",
    "electron-menubar": "^1.0.1",
    "electron-store": "^2.0.0",
    "fs-extra": "^7.0.0",
    "go-ipfs-dep": "^0.4.17",
    "ipfs": "^0.32.2",
    "ipfs-api": "^24.0.2",
    "ipfs-css": "^0.10.0",
    "ipfsd-ctl": "^0.39.2",
    "is-ipfs": "^0.4.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "shortid": "^2.2.13",
    "tachyons": "^4.11.1",
    "winston": "^3.1.0"
  }
}
